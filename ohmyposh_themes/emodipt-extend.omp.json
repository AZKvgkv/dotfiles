{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "newline": false,
      "segments": [
        {
          "foreground": "#E5C07B",
          "properties": {
            "time_format": "15:04"
          },
          "style": "plain",
          "template": "[{{ .CurrentDate | date .Format }}]",
          "type": "time"
        },
        {
          "foreground": "#E06C75",
          "properties": {
            "mapped_shell_names": {
              "bash": "Bash",
              "cmd": "Cmd",
              "powershell": "Shell",
              "pwsh": "Shell"
            }
          },
          "style": "plain",
          "template": " \udb82\udce8 {{ .Name }}",
          "type": "shell"
        },
        {
          "foreground": "#F3C267",
          "foreground_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#FF9248{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#ff4500{{ end }}",
            "{{ if gt .Ahead 0 }}#B388FF{{ end }}",
            "{{ if gt .Behind 0 }}#B388FF{{ end }}"
          ],
          "properties": {
            "branch_template": "{{ trunc 25 .Branch }}",
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "style": "plain",
          "template": "{{ if regexMatch \"github\" .UpstreamURL }} \uf408 {{ else if regexMatch \"gitlab\" .UpstreamURL }} \uf296 {{ else if regexMatch \"bitbucket\" .UpstreamURL }} \uf171 {{ else }} \uf113 {{ end }} {{ .HEAD }}{{ if gt .StashCount 0 }} \uf01c{{ end }}",
          "type": "git"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "foreground": "#b8ff75",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#E06C75{{ end }}"
          ],
          "style": "plain",
          "template": " x{{ reason .Code }}",
          "type": "status"
        },
        {
          "foreground": "#b8ff75",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#E06C75{{ end }}"
          ],
          "properties": {
            "always_enabled": true,
            "style": "roundrock"
          },
          "style": "diamond",
          "template": " {{ .FormattedMs }} ",
          "type": "executiontime"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#61AFEF",
          "properties": {
            "style": "full"
          },
          "style": "plain",
          "template": "\ue5ff {{ .Path }}",
          "type": "path"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#E06C75",
          "style": "plain",
          "template": "\u0021",
          "type": "root"
        },
        {
          "foreground": "#E06C75",
          "style": "plain",
          "template": "\u276f",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 3
}
